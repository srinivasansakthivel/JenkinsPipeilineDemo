pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Build Stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deployment'
            }
        }
        stage('Test') {
            steps {
                echo 'Test Stage'
            }
        }
        stage('Release') {
            steps {
                echo 'Release Stage'
            }
        }
    }
}
